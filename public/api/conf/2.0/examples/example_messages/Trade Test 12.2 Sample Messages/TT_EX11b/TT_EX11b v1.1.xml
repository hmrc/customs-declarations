           		xsi:schemaLocation="urn:wco:datamodel:WCO:DEC-DMS:2 ../WCO_DEC_2_DMS.xsd "
           		>
		<!--
		SCENARIO
		Type D pre-lodged declaration - Rejected
		Invalid Location RMGRMGRMX
		Procedure code 1040
		No control,Inventory linked Type D Maritime declaration with one DUCR - Rejected
	 	Goods: Straw envelopes for bottles
		Data set category: B1
		Scenario steps: Submit Type D declaration with DUCR1
		Expected notifications: DMSREJ
		-->
		<!--This should be updated to today's date.-->
		<AcceptanceDateTime>
			<p1:DateTimeString formatCode="304">20201001080000+01</p1:DateTimeString>
		</AcceptanceDateTime>
		<!--Function Code of 9 to represent an original declaration.-->
		<FunctionCode>9</FunctionCode>
		<!--DE 2/5: The trader assigned reference to the declaration -->
		<FunctionalReferenceID>TC04_RoRo_Test_NI02</FunctionalReferenceID>
		<IssueDateTime>
			<p1:DateTimeString formatCode="304">20201001080000+01</p1:DateTimeString>
		</IssueDateTime>
		<!--DE 1/1: EX to represent an Export declaration.-->
		<!--DE 1/2: D to represent a Pre lodged declaration.-->
		<TypeCode>EXD</TypeCode>
		<!--DE 1/9: Total number of goods items on the declaration.-->
		<GoodsItemQuantity>1</GoodsItemQuantity>
		<!--DE 6/18: Total packages -->
		<TotalPackageQuantity>10</TotalPackageQuantity>
		<!--DE 7/14: Identity of active means of transport crossing the border.-->
		<BorderTransportMeans>
			<ID>CSCLGLOBE</ID>
			<IdentificationTypeCode>11</IdentificationTypeCode>
			<!--DE 7/15: Country code of the nationality of the active means of transport crossing the border, 
				in this case GB.-->
			<RegistrationNationalityCode>GB</RegistrationNationalityCode>
			<!--DE 7/4: ModeCode of 1 to represent Sea transpot as the mode of transport at the border.-->
			<ModeCode>1</ModeCode>
		</BorderTransportMeans>
		<Consignment>
			<!--DE 3/32: Carrier identification no. 
				This is Security and Safety Data.  -->
			<Carrier>
				<ID>GB025115100010</ID>
			</Carrier>
			<!--DE 4/2: S&S Data - D indicating other e.g. direct debit to cash account. 
				This is Security and Safety Data. -->
			<Freight>
				<PaymentMethodCode>D</PaymentMethodCode>
			</Freight>
			<!--DE 5/18: Countries of routing codes. GBCAUS Routing from GB to US via Canada. 
				This is Security and Safety Data. -->
			<Itinerary>
				<SequenceNumeric>1</SequenceNumeric>
				<RoutingCountryCode>GB</RoutingCountryCode>
			</Itinerary>
			<Itinerary>
				<SequenceNumeric>2</SequenceNumeric>
				<RoutingCountryCode>CA</RoutingCountryCode>
			</Itinerary>
			<Itinerary>
				<SequenceNumeric>3</SequenceNumeric>
				<RoutingCountryCode>US</RoutingCountryCode>
			</Itinerary>
		</Consignment>
		<!--DE 3/18: EORI number of the Declarant. -->
		<Declarant>
			<ID>GB427168118378</ID>
		</Declarant>
		<!--DE 5/12 Customs office of exit. Indicating Dover -->
		<ExitOffice>
			<ID>GB000040</ID>
		</ExitOffice>
		<!--DE 3/2: EORI number of the Exporter, the same as 3/18 -->
		<Exporter>
			<ID>GB427168118378</ID>
		</Exporter>
		<GoodsShipment>
			<!--DE 3/9: Consignee Name and Address (Mandatory when DE 3/10 is not completed). 
			This is Security and Safety Data. -->
			<Consignee>
				<Name>Packings Import Incorporated</Name>
				<Address>
					<CityName>New York</CityName>
					<CountryCode>US</CountryCode>
					<Line>1 Bags Avenue</Line>
					<PostcodeID>NY1 234</PostcodeID>
				</Address>
			</Consignee>
			<Consignment>
				<!--DE 7/2: 1 to indicate that the goods are transported in containers.-->
				<ContainerCode>1</ContainerCode>
				<!--DE 7/7: ID of the means of transport and identification type of the means of transport 
					(in this scenario, 11 indicating name of the seagoing vessel). -->
				<DepartureTransportMeans>
					<ID>CSCLGLOBE</ID>
					<IdentificationTypeCode>11</IdentificationTypeCode>
				</DepartureTransportMeans>
				<!--DE 5/23: Location of goods. This field is mandatory.
					Name RMGRMGRMX  indicates an INVALID the location code
					TypeCode A indicates frontier or frontier linked location.
					Address qualifier type U indicates where the goods location is identified using a UN/LOCODE.
					Address country GB indicates the country of the goods location as Great Britain. -->
				<GoodsLocation>
					<Name>RMGRMGRMX</Name>
					<TypeCode>A</TypeCode>
					<Address>
						<!--DE 5/23: Type of ID of the Location - U in this scenario for UN/LOCODE. -->
						<TypeCode>U</TypeCode>
						<!--DE 5/23: Country code of the country where the goods may be examined,
							GB in this scenario.-->
						<CountryCode>GB</CountryCode>
					</Address>
				</GoodsLocation>
				<TransportEquipment>
					<!--DE 7/10: Container identification number.-->
					<SequenceNumeric>1</SequenceNumeric>
					<ID>CONU1234567</ID>
					<!--DE 7/18: Seal number. This is Security and Safety Data. -->
					<Seal>
						<SequenceNumeric>1</SequenceNumeric>
						<ID>333</ID>
					</Seal>
				</TransportEquipment>
			</Consignment>
			<Destination>
				<!--DE 5/8: Country code of the destination country, in this case US. -->
				<CountryCode>US</CountryCode>
			</Destination>
			<!--DE 5/14: Country code for the country from which the goods were originally dispatched, 
				in this case GB. -->
			<ExportCountry>
				<ID>GB</ID>
			</ExportCountry>
			<GovernmentAgencyGoodsItem>
				<!--DE 1/6: Sequential number of the goods item.-->
				<SequenceNumeric>1</SequenceNumeric>
				<!--DE 8/6: Enter the statistical value for the goods item in GBP (£ sterling), irrespective of the 
					currency used in Invoice Currency. -->
				<StatisticalValueAmount currencyID="GBP">1000</StatisticalValueAmount>
				<!--DE 8/5 Nature of Transaction. Enter, a 2-digit code (second digit is optional)
					The single digit codes listed in column A of the table provided for under Article 10(2) 
					of Commission Regulation (EC) No 113/2010 shall be used.
					Member States may also provide for a second digit from the list in column B of that table to 
					be collected. 
					In this case:
					- First digit (1) represents 'Transactions involving actual or intended transfer of ownership
					from residents to non-residents against financial or other compensation'
					- Second digit (1) represents 'Outright purchase/sale' -->
				<TransactionNatureCode>11</TransactionNatureCode>
				<!--DE 2/2: Additional information. 00400 Indicating Self representation i.e. the declarant and the 
					exporter are the same party-->
				<AdditionalInformation>
					<StatementCode>00400</StatementCode>
					<StatementDescription>EXPORTER</StatementDescription>
				</AdditionalInformation>
				<Commodity>
					<!--DE 6/8: Description of the goods.-->
					<Description>Straw envelopes for bottles, made of certified staw in wraps of 1000</Description>
					<Classification>
						<!--DE 6/14: Commodity code - combined nomenclature code.-->
						<ID>46021910</ID>
						<IdentificationTypeCode>TSP</IdentificationTypeCode>
					</Classification>
					<Classification>
						<!--DE 6/17: Commodity code – National additional code.-->
						<ID>VATZ</ID>
						<IdentificationTypeCode>GN</IdentificationTypeCode>
					</Classification>
					<GoodsMeasure>
						<!--DE 6/5: Gross mass of the goods in kilograms including packaging.-->
						<GrossMassMeasure>10</GrossMassMeasure>
						<!--DE 6/1: Net mass of the goods in kilograms without any packaging.-->
						<NetNetWeightMeasure>5</NetNetWeightMeasure>
					</GoodsMeasure>
				</Commodity>
				<GovernmentProcedure>
					<!--DE 1/10: Requested Procedure of 10 40 indicating Permanent export of goods in 
					free circulation and home use. -->
					<CurrentCode>10</CurrentCode>
					<PreviousCode>40</PreviousCode>
				</GovernmentProcedure>
				<GovernmentProcedure>
					<!--DE 1/11: 000 to indicate no additional procedure. -->
					<CurrentCode>000</CurrentCode>
				</GovernmentProcedure>
				<Packaging>
					<SequenceNumeric>1</SequenceNumeric>
					<!--DE 6/11: Shipping marks on the packages.-->
					<MarksNumbersID>2379261ETT</MarksNumbersID>
					<!--DE 6/10: Number of packages, in this case 10.-->
					<QuantityQuantity>10</QuantityQuantity>
					<!--DE 6/9: Type of packages - PK to represent standard packaging unit. -->
					<TypeCode>PK</TypeCode>
				</Packaging>
			</GovernmentAgencyGoodsItem>
			<!--DE 2/1: Previous document to show the DUCR assigned to the consignment. 
				This is a mandatory field. -->
			<PreviousDocument>
				<CategoryCode>Z</CategoryCode>
				<ID>0GB427168118378-0110NI02</ID>
				<TypeCode>DCR</TypeCode>
				<LineNumeric>1</LineNumeric>
			</PreviousDocument>
		</GoodsShipment>
	</Declaration>
