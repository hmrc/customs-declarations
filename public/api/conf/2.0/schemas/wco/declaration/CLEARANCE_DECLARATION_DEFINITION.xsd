<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="urn:wco:datamodel:WCO:DEC-DMS:2" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:ds="urn:wco:datamodel:WCO:Declaration_DS:DMS:2" targetNamespace="urn:wco:datamodel:WCO:DEC-DMS:2"
	elementFormDefault="qualified" version="2.3">
	<xs:import namespace="urn:wco:datamodel:WCO:Declaration_DS:DMS:2"
		schemaLocation="WCO_DS/CLEARANCE_TYPES.xsd" />
	<xs:element name="Declaration" type="Declaration" />
	<xs:complexType name="Declaration">
		<xs:sequence>
			<xs:element name="FunctionalReferenceID" type="ds:DeclarationFunctionalReferenceIDType"
						minOccurs="0" />
			<xs:element name="TypeCode" type="ds:DeclarationTypeCodeType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="GoodsItemQuantity" type="ds:DeclarationGoodsItemQuantityType" minOccurs="0" />
			<xs:element name="TotalGrossMassMeasure" type="ds:DeclarationTotalGrossMassMeasureType"
						minOccurs="0">
			</xs:element>
			<xs:element name="SpecificCircumstancesCodeCode"
				type="ds:DeclarationSpecificCircumstancesCodeCodeType" minOccurs="0" />
			<xs:element name="AdditionalDocument" minOccurs="0"
						maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CategoryCode"
									type="ds:AdditionalDocumentCategoryCodeType" minOccurs="0">
						</xs:element>
						<xs:element name="ID"
									type="ds:AdditionalDocumentIdentificationIDType" minOccurs="0">
						</xs:element>
						<xs:element name="TypeCode" type="ds:AdditionalDocumentTypeCodeType"
									minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Agent" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Name" type="ds:AgentNameTextType"
									minOccurs="0" />
						<xs:element name="ID" type="ds:AgentIdentificationIDType"
									minOccurs="0" />
						<xs:element name="FunctionCode" type="ds:AgentFunctionCodeType"
									minOccurs="0" />
						<xs:element name="Address" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CityName" type="ds:AddressCityNameTextType"
												minOccurs="0">
									</xs:element>
									<xs:element name="CountryCode" type="ds:AddressCountryCodeType"
												minOccurs="0">
									</xs:element>
									<xs:element name="CountrySubDivisionCode"
												type="ds:AddressCountrySubDivisionCodeType" minOccurs="0">
									</xs:element>
									<xs:element name="CountrySubDivisionName"
												type="ds:AddressCountrySubDivisionNameTextType" minOccurs="0">
									</xs:element>
									<xs:element name="Line" type="ds:AddressLineTextType"
												minOccurs="0">
									</xs:element>
									<xs:element name="PostcodeID" type="ds:AddressPostcodeIDType"
												minOccurs="0">
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AuthorisationHolder" minOccurs="0"
						maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID"
									type="ds:AuthorisationHolderIdentificationIDType" minOccurs="0">
						</xs:element>
						<xs:element name="CategoryCode"
									type="ds:AuthorisationHolderCategoryCodeType" minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BorderTransportMeans" minOccurs="0"
						maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ModeCode" type="ds:BorderTransportMeansModeCodeType"
									minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Declarant" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID" type="ds:DeclarantIdentificationIDType"
									minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExitOffice" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID" type="ds:ExitOfficeIdentificationIDType"
									minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Exporter" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Name" type="ds:ExporterNameTextType"
									minOccurs="0">
						</xs:element>
						<xs:element name="ID" type="ds:ExporterIdentificationIDType"
									minOccurs="0">
						</xs:element>
						<xs:element name="Address" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CityName" type="ds:AddressCityNameTextType"
												minOccurs="0" />
									<xs:element name="CountryCode" type="ds:AddressCountryCodeType"
												minOccurs="0" />
									<xs:element name="CountrySubDivisionCode"
												type="ds:AddressCountrySubDivisionCodeType" minOccurs="0" />
									<xs:element name="CountrySubDivisionName"
												type="ds:AddressCountrySubDivisionNameTextType" minOccurs="0" />
									<xs:element name="Line" type="ds:AddressLineTextType"
												minOccurs="0" />
									<xs:element name="PostcodeID" type="ds:AddressPostcodeIDType"
												minOccurs="0" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Consignee" minOccurs="0"
						maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID"
									type="ds:ConsigneeIdentificationIDType" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Consignor" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID" type="ds:ConsignorIdentificationIDType"
									minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GoodsShipment" minOccurs="0" maxOccurs="1">
				<!-- todo mc copy paste from temp xsd -->
			</xs:element>
			<xs:element name="ObligationGuarantee" minOccurs="0"
						maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ReferenceID"
									type="ds:ObligationGuaranteeReferenceIDType" minOccurs="0">
						</xs:element>
						<xs:element name="SecurityDetailsCode"
									type="ds:ObligationGuaranteeSecurityDetailsCodeType" minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PresentationOffice" minOccurs="0"
						maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID"
									type="ds:PresentationOfficeIdentificationIDType" minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SupervisingOffice" minOccurs="0"
						maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ID"
									type="ds:SupervisingOfficeIdentificationIDType" minOccurs="0">
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

		</xs:sequence>
	</xs:complexType> <!-- todo mc cleanup - single tag elements and mxOccurs remove, delete annotations -->
</xs:schema>