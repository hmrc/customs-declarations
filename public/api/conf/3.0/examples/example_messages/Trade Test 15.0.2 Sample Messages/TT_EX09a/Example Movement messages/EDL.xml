<?xml version="1.0" encoding="UTF-8"?>
<!-- EDL â€“ Movement message used for creating Goods Departure messages. This also covers the removal of goods back inland where an export is abandoned (Departures) -->
<inv:inventoryLinkingMovementRequest xmlns:inv="http://gov.uk/customs/inventoryLinking/v1">
	<inv:messageCode>EDL</inv:messageCode>
	<!--***
		Note that as per the CDS ILE Technical Completion Matrix for Trade Test, the AgentDetails fields are not used for inventory linking messages. These fields will be removed in the next version of this sample.
		***-->
	<inv:agentDetails>
		<!-- EORI number of the Community Systems Providers (CSPs). -->
		<inv:EORI>GB025115166433</inv:EORI>
		<inv:agentLocation>a</inv:agentLocation>
		<inv:agentRole>a</inv:agentRole>
	</inv:agentDetails>
	<inv:ucrBlock>
		<!--Air time MUCR assigned to the consignment. Depart at MUCR level -->
		<inv:ucr>A:14072020100</inv:ucr>
		<inv:ucrType>M</inv:ucrType>
	</inv:ucrBlock>
	<!-- ID to give a unique position of the location. CBRLHRXAD Indicating DEP Cargo, Heathrow. -->
	<inv:goodsLocation>GBAUCBRLHRXAD</inv:goodsLocation>
	<inv:goodsArrivalDateTime>2020-07-14T11:03:00</inv:goodsArrivalDateTime>
	<!-- This should be today's Date -->
	<inv:goodsDepartureDateTime>2020-07-14T11:10:00</inv:goodsDepartureDateTime>
	<inv:movementReference>Movement15</inv:movementReference>
	<inv:transportDetails>
		<inv:transportID>11CSCLGLOBE</inv:transportID>
		<inv:transportMode>1</inv:transportMode>
		<inv:transportNationality>GB</inv:transportNationality>
	</inv:transportDetails>
</inv:inventoryLinkingMovementRequest>
