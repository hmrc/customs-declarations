<!-- EAL â€“ Movement message notification format for Entry arriving at location (Arrivals) -->
<inv:inventoryLinkingMovementRequest xmlns:inv="http://gov.uk/customs/inventoryLinking/v1">
	<inv:messageCode>EAL</inv:messageCode>
	<!--***
		Note that as per the CDS ILE Technical Completion Matrix for Trade Test, the AgentDetails fields are not used for inventory linking messages. These fields will be removed in the next version of this sample.
		***-->
	<inv:agentDetails>
		<!-- EORI number of the Community Systems Providers (CSPs). -->
		<inv:EORI>GB025115166433</inv:EORI>
		<inv:agentLocation>a</inv:agentLocation>
		<inv:agentRole>a</inv:agentRole>
	</inv:agentDetails>
	<inv:ucrBlock>
		<!--Air time MUCR assigned to the consignment. Arriving at MUCR level. This value has come from Declaration -->
		<inv:ucr>1GB427168118378-0502ST05</inv:ucr>
		<inv:ucrType>D</inv:ucrType>
	</inv:ucrBlock>
	<!-- ID to give a unique position of the location. 'SOUSTNSOU' Indicating Southampton Docks -->
	<inv:goodsLocation>GBAULGWLGWLGW</inv:goodsLocation>
	<!-- This should be today's Date -->
	<inv:goodsArrivalDateTime>2021-02-05T08:00:00</inv:goodsArrivalDateTime>
	<!--  <inv:goodsDepartureDateTime>2020-11-26T08:02:00</inv:goodsDepartureDateTime> -->
	<inv:movementReference>Movement14</inv:movementReference>
	<inv:transportDetails>
		<inv:transportID>11CSCLGLOBE</inv:transportID>
		<inv:transportMode>1</inv:transportMode>
		<inv:transportNationality>GB</inv:transportNationality>
	</inv:transportDetails>
</inv:inventoryLinkingMovementRequest>
